{
  "name": "line-prophunt",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev -w packages/worker",
    "admin": "npm run dev -w packages/admin-ui",
    "build": "npm run build -w packages/admin-ui",
    "deploy": "npm run build -w packages/admin-ui && npm run deploy -w packages/worker",
    "db:migrate": "npm run db:migrate -w packages/worker",
    "db:migrate:remote": "npm run db:migrate:remote -w packages/worker"
  }
}
